{
  "name": "@airgram/example-rollup-config",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "check": "tsc --noEmit",
    "clean": "rm -rf ./dist",
